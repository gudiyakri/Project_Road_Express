<% include ../partials/header %> <% include ../partials/sidebar %>

<div class="page-wrapper">
	<div class="container-fluid">
		<% include ../partials/message %>
		<div class="row page-titles">
			<div class="col-md-6 col-8 align-self-center">
				<h3 class="text-themecolor mb-0 mt-0">Dashboard</h3>
				<ol class="breadcrumb">
					<li class="breadcrumb-item"><a href="javascript:void(0)">Operations</a></li>
					<li class="breadcrumb-item active">Vehicles info</li>
				</ol>
			</div>
		</div>
		<div class="card">
			<div class="card-body">
				<div class="row">
					<p class="col-2">Vehicle Number</p>
					<p class="col-2"><%= vehicle[0].v_num %></p>
				</div>
				<div class="row">
					<p class="col-2">Vehicle Type</p>
					<p class="col-2"><%= vehicle[0].v_type %></p>
				</div>
				<div class="row">
					<p class="col-2">Transporter Name</p>
					<p class="col-2"><%= vehicle[0].name %></p>
				</div>
				<div class="row">
					<p class="col-2">Insurance Number</p>
					<p class="col-2"><%= vehicle[0].mob %></p>
				</div>
				<div class="row">
					<p class="col-2">Verify date</p>
					<p class="col-2"><%= vehicle[0].verif_flag_date %></p>
				</div>
				<div class="row">
					<p class="col-2">Permit Type</p>
					<p class="col-2"><%= vehicle[0].permit_type %></p>
				</div>
				<div class="row">
					<p class="col-2">Rc Expire</p>
					<p class="col-2"><%= vehicle[0].rc_exp %></p>
				</div>
				<div class="row">
					<p class="col-2">Vehicle Pic</p>
					<a target="_blank" href="/admin/display/vehicle/<%= vehicle[0].pic_v %>"
						><img src="/images/vehicle/<%= vehicle[0].pic_v %>" style="width: 50px; height: 60px"
					/></a>
				</div>
				<div class="row">
					<p class="col-2">Rc Font</p>
					<a target="_blank" href="/admin/display/front/<%= vehicle[0].pic_rc_front %>"
						><img src="/images/rc_front/<%= vehicle[0].pic_rc_front %>" style="width: 50px; height: 60px"
					/></a>
				</div>
				<div class="row">
					<p class="col-2">Rc Back</p>
					<a target="_blank" href="/admin/display/back/<%= vehicle[0].pic_rc_back %>"
						><img src="/images/rc_back/<%= vehicle[0].pic_rc_back %>" style="width: 50px; height: 60px"
					/></a>
				</div>
				<div class="row">
					<p class="col-2">Insurance</p>
					<a target="_blank" href="https://admin.roadexpress.in/images/insurance_pic/<%= vehicle[0].insurance_pic %>"
						><img src="/images/insurance_pic/<%= vehicle[0].insurance_pic %>" style="width: 50px; height: 60px"
					/></a>
				</div>
				<div class="row">
					<p class="col-2">Permit</p>
					<a target="_blank" href="https://admin.roadexpress.in/images/permit_pic/<%= vehicle[0].permit_pic %>"
						><img src="/images/permit_pic/<%= vehicle[0].permit_pic %>" style="width: 50px; height: 60px"
					/></a>
				</div>
			</div>
		</div>
		<footer class="footer">Â© 2019 Road Express by CodeBuckets.in</footer>
	</div>
</div>

<% include ../partials/footer %>
